# Programs on dictionaries
rivers = {'Nile': 'Egypt', 'Amazon':'Brazil', 'Ganga':'India'}
for river,country in rivers.items():
    print(f"{river.title()} runs through {country.title()}")

print('\n')
for river in rivers.keys():
    print(river.title())

print('\n')


for country in rivers.values():
    print(country.title())
